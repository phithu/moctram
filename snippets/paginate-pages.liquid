<nav class="clearfix relative nav_pagi w_100">
	<ul class="pagination clearfix float-right">
		{% if paginate.previous %}
		<li class="page-item hidden-xs">
			<a class="page-link" href="{{ paginate.previous.url }}" data-page='{{paginate.current_page | minus: 1}}'>
				<i class="fa fa-angle-left" aria-hidden="true"></i>
			</a></li>
		{% else %}  
		<li class="page-item disabled"><a class="page-link" href="#"><i class="fa fa-angle-left"></i></a></li>
		{% endif %}

		{% for part in paginate.parts %}
		{% if part.is_link %}
		<li class="page-item">
			<a class="page-link" href="{{ part.url }}"  data-page='{{part.title}}'>{{ part.title }}</a>
		</li>
		{% else %}
		{% if part.title == paginate.current_page %}
		<li class="active page-item disabled">
			<a class="page-link" href="#">{{ part.title }}</a>
		</li>
		{% else %}
		<li class="page-item">
			<a class="page-link" href="{{ part.url }}" data-page='{{part.title}}'>{{ part.title }}</a>
		</li>
		{% endif %}
		{% endif %}
		{% endfor %}

		{% if paginate.next %}
		<li class="page-item hidden-xs">
			<a class="page-link" href="{{ paginate.next.url }}" data-page='{{paginate.current_page | plus: 1}}'>
				<i class="fa fa-angle-right" aria-hidden="true"></i>
			</a>
		</li>
		{% else %}
		<li class="page-item disabled"><a class="page-link" href="#"><i class="fa fa-angle-right"></i></a></li>
		{% endif %}
	</ul>
</nav>