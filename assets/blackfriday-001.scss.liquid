@charset "UTF-8";

/* ==========  VARIABLES  ========== */
$prefix: 'ega-';
{%assign base_font = settings.blackfriday_001__theme__font | remove: 'Google+' | replace: '+' , ' '%}


$primary-font: '{{ base_font }}', "Helvetica", "Arial", sans-serif;

$main_color : {{ settings.blackfriday_001__theme__main_color }};
$bg_color: {{ settings.blackfriday_001__theme__bg }};
$title_color : {{ settings.blackfriday_001__theme__title_color }};
$sale_color: {{ settings.blackfriday_001__theme__sale_color }};
$clock_bg : {{ settings.blackfriday_001__clock__bg }};
$clock_color : {{ settings.blackfriday_001__clock__color }};
$trend_bg : {{ settings.blackfriday_001__carousel__bg }};
$footer_bg : {{ settings.blackfriday_001__footer__bg }};
$footer_color : {{ settings.blackfriday_001__footer__color }};

/* ==========  COMMON  ========== */
html {
   font-size: 1em;
   line-height: 1.5;
   width: 100%;
   height: 100%;
   -ms-touch-action: manipulation;
   touch-action: manipulation;
}

body {
   width: 100%;
   min-height: 100%;
   margin: 0;
   background: $bg_color;
   &.menu-toggled{
      overflow-y: hidden; 
   }
}
*{
   box-sizing: border-box;
}
main { display: block; }

::-moz-selection {
   background: #b3d4fc;
   text-shadow: none;
}
::selection {
   background: #b3d4fc;
   text-shadow: none;
}
a, button {
   &:focus {
      outline: none;
   }
}

audio, canvas, iframe, img, svg, video { vertical-align: middle }

textarea { resize: vertical; }

img, iframe { max-width: 100%; }

h1, h2, h3, h4, h5, h6, p {
   margin: 0;
   padding: 0;
   font-weight: 400;
}

ul{
   padding: 0;
   margin: 0;
   list-style-type: none !important;
}

.text-color{
   &--success{
      color: #3c763d;
   }
   &--danger{
      color: #a94442;
   }
}

.clearfix::after {
  display: block;
  clear: both;
  content: "";
}

.ega-color--initial{
	color: initial !important;
}

.ega-btn {
	display: inline-block;
	padding: 5px 20px;
	border-radius: 20px;
	border: 1px solid transparent;
	-webkit-transition: all 0.5s ease-in-out;
	-o-transition: all 0.5s ease-in-out;
	transition: all 0.5s ease-in-out;	
}

.ega-btn-color{
	background: $main_color;
	color: #fff;
	&--outline {
		background-color: transparent;
		border-color: $main_color;
		color: $main_color;
		&:hover {
			background-color: $main_color;
			border-color: $main_color;
			color: #fff;
		}
	}
}


/* ==========  HELPERS  ========== */
.#{$prefix}hidden { 
   display: none !important;
}

.#{$prefix}inline-block { 
   display: inline-block !important;
}

.#{$prefix}block { 
   display: block !important;
}

.#{$prefix}hidden { 

   &--sm {
      @media(min-width: 576px) {
         display: none!important;
      }
   }

   &--md {
      @media(min-width: 768px) {
         display: none!important;
      }
   }

   &--lg {
      @media(min-width: 992px) {
         display: none!important;
      }
   }

   &--xl {
      @media(min-width: 1200px) {
         display: none!important;
      }
   }
}

.#{$prefix}inline-block { 

   &--sm {
      @media(min-width: 576px) {
         display: inline-block!important;
      }
   }

   &--md {
      @media(min-width: 768px) {
         display: inline-block!important;
      }
   }

   &--lg {
      @media(min-width: 992px) {
         display: inline-block!important;
      }
   }

   &--xl {
      @media(min-width: 1200px) {
         display: inline-block!important;
      }
   }
}

.#{$prefix}block { 

   &--sm {
      @media(min-width: 576px) {
         display: block!important;
      }
   }

   &--md {
      @media(min-width: 768px) {
         display: block!important;
      }
   }

   &--lg {
      @media(min-width: 992px) {
         display: block!important;
      }
   }

   &--xl {
      @media(min-width: 1200px) {
         display: block!important;
      }
   }
}

.#{$prefix}flex { 

   &--sm {
      @media(min-width: 576px) {
         display: flex!important;
      }
   }

   &--md {
      @media(min-width: 768px) {
         display: flex!important;
      }
   }

   &--lg {
      @media(min-width: 992px) {
         display: flex!important;
      }
   }

   &--xl {
      @media(min-width: 1200px) {
         display: flex!important;
      }
   }
}


/* ==========  TYPOGRAPHY  ========== */
html, body {
   font-family: $primary-font;
}
p {
   font-weight: 400;
   letter-spacing: 0;
   margin-bottom: 16px;
}
a {
   text-decoration: none;
}
/* ==========  GRID  ========== */
.#{$prefix}container {
   padding-left: 15px;
   padding-right: 15px;
   width: 100%;
   margin: 0 auto;

  &--fluid{
    width: 100%;
    padding-right: 15px;
    padding-left: 15px;
    margin-right: auto;
    margin-left: auto;
  }

   @media (min-width: 576px) {
      max-width: 540px;
   }

   @media (min-width: 768px) {
      max-width: 720px;
   }

   @media (min-width: 992px) {
      max-width: 960px;
   }

   @media (min-width: 1200px) {
      max-width: 1140px;
   }
}
.#{$prefix}row {
   display: flex;
   flex-wrap: wrap;
   margin-left: -15px;
   margin-right: -15px;
}
.#{$prefix}col, [class*=#{$prefix}col-] {
   padding-left: 15px;
   padding-right: 15px;
}

/** Break point **/
$breakpoint_sm: (576px, sm);
$breakpoint_md: (768px, md);
$breakpoint_lg: (992px, lg);
$breakpoint_xl: (1200px, xl);

/** Collumn **/


@mixin ega_col_flex() {
   -ms-flex-preferred-size: 0; flex-basis: 0; -ms-flex-positive: 1;  flex-grow: 1; max-width: 100%;
}
@mixin ega_col($i, $collumns) {
   -ms-flex: 0 0 percentage($i / $collumns);
   flex: 0 0 percentage($i / $collumns);
   max-width: percentage($i / $collumns);
}

.#{$prefix}col {
   @include ega_col_flex();
}

@for $i from 1 through 12 {
   .#{$prefix}col-#{$i} {
      @include ega_col($i, 12);
   }
}

@each $i in 20, 30, 40, 60, 70, 80, 90 {
   .#{$prefix}col-#{$i} {
      @include ega_col($i, 100);
   }
}

@each $breakpoint, $infix in $breakpoint_sm, $breakpoint_md, $breakpoint_lg, $breakpoint_xl {
   @media (min-width: $breakpoint) {

      .#{$prefix}col-#{$infix} {
         @include ega_col_flex();
      }

      @for $i from 1 through 12 {
         .#{$prefix}col-#{$infix}-#{$i} {
            @include ega_col($i, 12);
         }
         .#{$prefix}order-#{$infix}-#{$i} {
            -ms-flex-order: $i;
            order: $i;
         }
   }

   .#{$prefix}col-ten {
      @include ega_col(10, 100);
   }

   @each $i in 20, 30, 40, 60, 70, 80, 90 {
      .#{$prefix}col-#{$infix}-#{$i} {
         @include ega_col($i, 100);
      }
}

}
}

/* ==========  MIXINS  ========== */

@mixin translateX($v) {
   -webkit-transform: translateX($v);
   -ms-transform: translateX($v);
   -o-transform: translateX($v);
   transform: translateX($v);
}

@mixin translateY($v) {
   -webkit-transform: translateY($v);
   -ms-transform: translateY($v);
   -o-transform: translateY($v);
   transform: translateY($v);
}

@mixin translate3d($x,$y) {
   -webkit-transform: translate3d($x,$y,0);
   -ms-transform: translate3d($x,$y,0);
   -o-transform: translate3d($x,$y,0);
   transform: translate3d($x,$y,0);
}

@mixin transform($values...){
   @each $var in $values {
      -webkit-transform: #{$var};
      -o-transform: #{$var};
      transform: #{$var};
   }
}

@mixin transition($values...){
   @each $var in $values {
      -webkit-transition: #{$var};
      -o-transition: #{$var};
      transition: #{$var};
   }
}

/** END GLOBAL CSS **/

/** SALE PAGE **/
.button__action{
  display: inline-block;
  background: #eee;
  padding: 5px 20px;
  border-radius: 20px;
  color: initial;
  @include transition(all 0.5s ease-in-out);
  @media(max-width: 767px){
    font-size: 12px;
  }
  &:hover{
    background: $main_color;
    color: #fff;
  }
}

header {
  text-align: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 999;
  height: 50px;
  padding: 5px 0;
  transform: translate3d(0px, -50px, 0px);
  -webkit-transition: transform .3s; /* Safari */
  transition: transform .3s;
  background-color: rgba(22, 22, 22,.5);
  &.fixed_header {
    transform: translate3d(0px, 0px, 0px);
  }
  #back-to-home{
    position: absolute;
    top: 0;
    left: 0;
    color: #fff;
    font-size: 30px;
    height: 50px;
    line-height: 50px;
    width: 50px;
  }
  #logo img {
    height: 40px;
  }
}

.home-banner{
  position: relative;
  min-height: 300px;
	& > img{
		width: 100%;
	}
	@media(max-width: 767px){
		min-height: 100px;
	}
  #countdown{
    position: absolute;
    bottom: 100px;
    left: 46%;
    @include translateX(-46%);
    margin: 0;
    display: inline-block;
    width: auto;
    @media(max-width: 1024px){
      width: 700px;
	  bottom: 55px;
    }
    @media(max-width: 767px){
      bottom: 0;
    }
  }
  .next-section{
    position: absolute;
    font-size: 40px;
    color: #fff;
    bottom: 0;
    left: 50%;
    margin-left: -30px;
    font-size: 60px;
    text-shadow: #030303 0px 4px 4px;
    -webkit-animation-duration: 1s;
    animation-duration: 1s;
    -webkit-animation-fill-mode: both;
    animation-fill-mode: both;
    -webkit-animation-name: fadeInDown;
    animation-name: fadeInDown;
    -webkit-animation-iteration-count: infinite;
    animation-iteration-count: infinite;
    -webkit-animation-duration: 1.5s !important;
    animation-duration: 1.5s !important;
  }
	@media(max-width: 320px){
		padding-bottom: 30px;
		background: black;
		#countdown{
			bottom: 10px;
		}
	}
}

.flash-hover{
  position: relative;
  display: block;
  overflow: hidden;
  &:before {
    position: absolute;
    z-index: 1;
    top: 0;
    left: -10%;
    width: 120%;
    height: 100%;
    background: rgba(255, 255, 255, 0.2);
    content: '';
    -webkit-transition: -webkit-transform .6s;
    transition: transform .8s;
    -webkit-transform: scale3d(1.9, 1.4, 1) rotate3d(0, 0, 1, 45deg) translate3d(0, 100%, 0);
    transform: scale3d(1.9, 1.4, 1) rotate3d(0, 0, 1, 135deg) translate3d(0, 100%, 0);
    opacity: 0;
    filter: alpha(opacity=0);
    visibility: hidden;
    -webkit-box-sizing: border-box;
    -moz-box-sizing: border-box;
    box-sizing: border-box;
  }
  &:hover:before {
    -webkit-transform: scale3d(1.9, 1.4, 1) rotate3d(0, 0, 1, 45deg) translate3d(0, -100%, 0);
    transform: scale3d(1.9, 1.4, 1) rotate3d(0, 0, 1, 135deg) translate3d(0, -100%, 0);
    opacity: 1;
    filter: alpha(opacity=100);
    visibility: initial;
  }
}

