.product{
  position: relative;
  display: block;
  background: #ffffff;
  margin-bottom: 30px;
  text-align: center;
  border: 1px solid transparent;
  @media(max-width: 767px){
    margin-bottom: 20px;
  }
  &:hover{
    border-color: $main_color;
  }
  &__title{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    color: initial; 
    margin-bottom: 10px;
    @media(max-width: 767px){
      font-size: 12px;
    }
  }
  &__info{
    padding: 15px;
    @media(max-width: 767px){
      padding: 5px;
    }
  }
  &__price{
    margin-bottom: 10px;
    @media(max-width: 767px){
      font-size: 12px;
    }
    ins{
      color: $sale_color;
      text-decoration: none;
    }
    del{
      color: #999;
    }
  }
  &__sale{
    position: absolute;
    width: 80px;
    text-align: center;
    top: 0;
    right: 0;
    padding: 5px;
    background: $sale_color;
    color: #fff;
    @media(max-width: 767px){
      width: 42px;
      font-size: 12px;
      .triangle {
        display: none;
      }
    }
    .triangle {
      position: absolute;
      top: 100%;
      left: 0;
      width: 100%;
      height: 60px;
      overflow: hidden;
      &--left {
        z-index: 1;
        &:before {
          top: 0;
          left: 10px;
          border-left: 80px solid rgba(0, 0, 0, 0.2);
          border-bottom: 30px solid transparent;
        }
        &:after {
          left: 0;
          border-left: 80px solid $sale_color;
          border-bottom: 20px solid transparent;
        }
      }
      &--right{
        &:before {
          top: 10px;
          left: 0;
          border-right: 80px solid rgba(0, 0, 0, 0.2);
          border-bottom: 20px solid transparent;
        }
        &:after {
          left: 0;
          border-right: 80px solid $sale_color;
          border-bottom: 20px solid transparent;
        }
      }
      &:before, &:after {
        position: absolute;
        width: 0;
        height: 0;
        content: "";
        top: 0;
      }
    }
  }

	&__label{
		position: absolute;
		width: 50px;
		text-align: center;
		top: 0;
		right: 0;
		padding: 5px;
		background: $sale_color;
		color: #fff;
